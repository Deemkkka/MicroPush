<Window x:Class="MicroPush.optionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MicroPush"
        mc:Ignorable="d"
        Title="optionsWindow" Height="450" Width="800" WindowState="Normal" WindowStartupLocation="CenterScreen">
    <StackPanel Background="#FF1C1F25">
        <TextBlock FontSize="36" HorizontalAlignment="Center" Foreground="White" FontFamily="Bauhaus 93" FontWeight="Normal" TextDecorations="{x:Null}">Параметры игры</TextBlock>
        <Button x:Name="btnSecond" Content="Вернуться" Click="btnSecond_Click" Background="White" FontSize="20" FontFamily="Times New Roman" UseLayoutRounding="True" FlowDirection="LeftToRight" HorizontalAlignment="Center"/>
    </StackPanel>
</Window>
